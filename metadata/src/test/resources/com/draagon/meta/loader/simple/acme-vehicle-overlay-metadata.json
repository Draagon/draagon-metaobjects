            }
          },
          {"field": {"name": "specialFeatures", "override": true,
              "children": [{"attr": {"name": "specialConfig", "type": "string", "@feature1": "climate-controlled", "@feature2": "security-system", "@maxFeatures": "10"}}]
            }
          }
        ]
      }
    },
    {"object": {"package": "::vehicle", "name": "Vehicle",
        "children": [
          {"field": {"name": "warranty", "type": "boolean", "@default": "true"}},
          {"field": {"name": "inspectionDate", "type": "date",
              "children": [
                {"validator": {"name": "pastDate", "type": "required", "@maxDate": "today"}}
              ]
            }
          }
        ]
      }
    },
    {"object": {"package": "::vehicle::car::luxury", "name": "Porsche",
