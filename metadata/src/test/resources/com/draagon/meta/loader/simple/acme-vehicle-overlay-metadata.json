          {"field": {"name": "specialEdition", "override": true,
              "children": [
                {"validator": {"name": "editionValidator", "type": "length", "@min": "0", "@max": "50"}},
                {"attr": {"name": "editionDetails", "type": "string", "@limitedRun": "true", "@serialNumber": "", "@collectible": "false"}}
              ]
            }
          }
        ]
      }
    },
    {"object": {"package": "::vehicle::motorcycle", "name": "Motorcycle",
        "children": [
          {"field": {"name": "customPaint", "type": "boolean", "@default": "false"}},
          {"field": {"name": "modifications", "override": true,
              "children": [{"validator": {"name": "modValidator", "type": "length", "@max": "200"}}]
            }
          }
        ]
      }
    }
  ]
}}